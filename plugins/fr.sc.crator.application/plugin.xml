         id="application"
         point="org.eclipse.core.runtime.applications">
      <application>
         <run
               class="fr.sc.crator.application.Application">
         </run>
      </application>
   </extension>
   <extension
         id="CRAtor"
         point="org.eclipse.core.runtime.products">
